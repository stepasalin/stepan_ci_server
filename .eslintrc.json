{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "google",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "no-else-return": "error",
    "prettier/prettier": "error",
    "require-jsdoc": "off"
  }
}